<UserControl x:Class="TabNavApp.API.TagSearchLBItem"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
   Height="72" Width="Auto" d:DesignWidth="413" xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk" d:DesignHeight="96" BorderBrush="#FF1D1D1D">

    <Grid x:Name="LayoutRoot" Background="White" DataContext="{Binding}" Height="72" Width="Auto">
        <TextBlock FontFamily="Verdana" Text="{Binding prefLabel}" Margin="12,0,66,0" Foreground="#FF4F894F" FontSize="16" VerticalAlignment="Top" Height="26" Name="prefLabelTB"></TextBlock>
        <Button Height="48" HorizontalAlignment="Right" Name="button1" VerticalAlignment="Bottom" Width="48" BorderThickness="0" Style="{StaticResource TagButton}" Margin="0,0,7,11">
        </Button>
        <TextBlock FontFamily="Verdana" Text="{Binding altLabels}" FontSize="10" Foreground="#FF484848" Height="22" Margin="12,28,66,0" Name="altLabelsTB" VerticalAlignment="Top" />
        <TextBlock FontFamily="Verdana" Text="{Binding description}" FontSize="10" Foreground="#FF484848" Height="22" Margin="12,50,66,0" Name="descriptionTB" VerticalAlignment="Top" />
    </Grid>
</UserControl>

